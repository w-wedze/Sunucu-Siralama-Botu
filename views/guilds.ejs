      <header class="navbar-expand-md">
        <div class="collapse navbar-collapse" id="navbar-menu">
        </div>
      </header>

      <div class="page-wrapper">
  
        

  <div class="page-wrapper">
    <div class="page-header d-print-none">
      <div class="container-xl">
        <div class="row g-2 align-items-center">
  <div class="col text-start">
    <h2 class="page-title">
      <div class="col">
        <h2 class="page-title">
         Sunucular
        </h2>
        <div class="text-muted mt-1"><a href="/" style="color: #2785FF;">Twix</a> <label style="color: white;">></label> Sunucular (<%= totalGuilds %>)</div>
      </div>
    </h2>
  </div>
</div>
</div>
</div>
  <hr /><br>

  <% if (botGuilds && botGuilds.length > 0) { %>
    <% const sortedGuilds = botGuilds.sort((a, b) => b.voiceChannelUsers - a.voiceChannelUsers); %>
    <div class="row mx-8">
      <% sortedGuilds.forEach(guild => { %>
        <div class="col-3 mb-4">
          <div class="card" style="position: relative; overflow: hidden; margin-bottom: 20px;">
            <div style="background-image: url('<%= guild.bannerURL %>'); background-size: cover; background-position: center center; height: 180px; display: flex; align-items: flex-end; justify-content: center; ">
              <span class="avatar avatar-xl mb-4 rounded">
                <img src="<%= guild.iconURL %>">
              </span>
            </div>
            <div class="card-body p-4 py-5 text-center" style="position: relative; z-index: 1;">
              <h3 class="mb-0"><a href="/guild/<%= guild.id %>"><%= guild.name %></a></h3><br>
              <div class="text-muted">Oluşturulma: <p style="color: white;"><%= guild.createdAt.toISOString() %></p></div>
              <div class="mb-3 d-flex justify-content-center">
                <div class="text-muted me-3">Üyeler <p style="color: white;"><%= guild.memberCount %></p></div>
                <div class="text-muted me-3">Oylar <p style="color: white;">0</p></div>
                <div class="text-muted">Ses <p style="color: white;"><%= guild.voiceChannelUsers %></p></div>
              </div>
              
                <a href="https://www.discord.gg/<%= guild.vanityURLCode %>" class="btn btn-outline-dark active w-100">
                Sunucuya Katıl
                </a>
              
            </div>
            <div class="progress card-progress">
              <div class="progress-bar" style="width: 100%" role="progressbar" aria-valuenow="38" aria-valuemin="0" aria-valuemax="100" aria-label="38% Complete">
                <span class="visually-hidden">38% Complete</span>
              </div>
            </div>
          </div>
        </div>
      <% }); %>
    </div>
  <% } else { %>
    <p>Herhangi bir sunucu listelenmedi.</p>
  <% } %>
  
        <footer class="footer footer-transparent d-print-none">
          <div class="container-xl">
            <div class="row text-center align-items-center flex-row-reverse">
              <div class="col-lg-auto ms-lg-auto">
                <ul class="list-inline list-inline-dots mb-0">
                  <li class="list-inline-item">
                    <a href="https://discord.com/users/1067476859933179954" target="_blank" class="link-secondary" rel="noopener">
                      <svg xmlns="http://www.w3.org/2000/svg" class="icon text-pink icon-filled icon-inline" width="24" height="24" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" fill="none" stroke-linecap="round" stroke-linejoin="round"><path stroke="none" d="M0 0h24v24H0z" fill="none"/><path d="M19.5 12.572l-7.5 7.428l-7.5 -7.428a5 5 0 1 1 7.5 -6.566a5 5 0 1 1 7.5 6.572" /></svg>
                      Sponsor
                    </a>
                  </li>
                </ul>
              </div>
              <div class="col-12 col-lg-auto mt-3 mt-lg-0">
                <ul class="list-inline list-inline-dots mb-0">
                  <li class="list-inline-item">
                    Tüm Hakları Saklıdır &copy;
                    <a href="." class="link-secondary">Twix</a>
                  </li>
                </ul>
              </div>
            </div>
          </div>
        </footer>
