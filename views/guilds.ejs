<script src="https://cdn.jsdelivr.net/npm/sweetalert2@10"></script>

<script>
  function showAlert() {
    Swal.fire({
      icon: 'warning',
      title: 'Oturum Açılamadı!',
      text: 'Oturum açma işlemi çok yakında.',
      confirmButtonText: 'Tamam'
    });
  }
</script>

